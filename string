#include <stdio.h>
#include <string.h>
int main()
{
    char a[20]="econsystem";
    char b[20]={'e','c','o','n','s','y','s','t','e','m','\0'};
    // using the %zu format specifier to print size_t
    printf("length of string a = %zu \n",strlen(a));
    printf("length of string b =  %zu \n",strlen(b));
    return 0;
}
